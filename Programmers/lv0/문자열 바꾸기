class Solution {
    public String solution(String my_string) {
        //String answer = "";
        char[] myChars = my_string.toCharArray();
        char[] answer = new char[myChars.length];
        for(int i=0; i < myChars.length; i++){
            answer[myChars.length-1-i] = myChars[i];
        }
        String result = String.valueOf(answer);
        return result;
    }
}